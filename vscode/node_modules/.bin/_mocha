#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules/mocha/bin/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules/mocha/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules/mocha/bin/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules/mocha/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/mocha@10.2.0/node_modules:/home/vishnumenon/Documents/lightrail/vscode/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../mocha/bin/_mocha" "$@"
fi
